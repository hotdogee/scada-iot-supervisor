    "@feathersjs/authentication-jwt": "^2.0.10",
    "@feathersjs/authentication-local": "^4.0.0-pre.5",
    "@feathersjs/authentication-oauth": "^4.0.0-pre.5",
    "@feathersjs/authentication-oauth2": "^1.3.1",
    "@feathersjs/configuration": "^4.0.0-pre.5",
    "@feathersjs/errors": "^4.0.0-pre.5",
    "@feathersjs/express": "^4.0.0-pre.5",
    "@feathersjs/feathers": "^4.0.0-pre.5",
    "@feathersjs/socketio": "^4.0.0-pre.5",
    "@feathersjs/socketio-client": "^1.2.1",
    "ajv": "^6.10.2",
    "axios": "^0.19.0",
    "common-password": "^0.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "feathers-authentication-hooks": "^0.3.1",
    "feathers-blob": "^2.1.0",
    "feathers-hooks-common": "^4.20.7",
    "feathers-logger": "^0.3.2",
    "feathers-mailer": "^3.0.1",
    "feathers-mongodb": "^5.0.0",
    "fs-blob-store": "^5.2.1",
    "helmet": "^3.20.0",
    "i18n": "^0.8.3",
    "lodash.merge": "^4.6.2",
    "mongodb": "^3.2.7",
    "mongodb-core": "^3.2.7",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "node-webcrypto-ossl": "^1.0.48",
    "nodemailer": "^6.3.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pug": "^2.0.4",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.23.0",
    "supports-color": "^7.0.0",
    "web-push": "^3.3.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "lint-staged": "^9.2.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  }
