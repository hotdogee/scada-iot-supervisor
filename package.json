{
  "name": "scada-iot-supervisor",
  "description": "SCADA/IoT Supervisor System",
  "version": "0.1.0",
  "homepage": "https://scada.hanl.in",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Han Lin",
    "email": "hotdogee@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "^12.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "test:all": "npm run eslint && cross-env NODE_ENV=test npm run mocha",
    "precommit": "lint-staged",
    "eslint": "eslint src/. test/. --config .eslintrc.js",
    "dev": "cross-env NODE_ENV=test DEBUG=*,-babel*,-engine*,-socket*,-eslint:*,-snapdragon*,-nodemon*,-express*,-@feathersjs/transport-commons nodemon src/",
    "dev:seed": "nodemon src/ --seed",
    "start": "node src/",
    "start:seed": "cross-env NODE_ENV=test node src/ --seed",
    "docker:compose": "npm i && cross-env DEBUG_COLORS=1 DEBUG=*,-engine*,-socket*,-eslint:*,-snapdragon*,-nodemon*,-express*,-@feathersjs/transport-commons nodemon src/ --color",
    "debugger": "cross-env NODE_ENV=test DEBUG=*,-engine*,-socket*,-eslint:*,-snapdragon*,-nodemon*,-express*,-@feathersjs/transport-commons node --nolazy --inspect-brk=9229 src/",
    "mocha": "mocha test/ --recursive --exit --timeout 10000",
    "feathers": "node ../feathers-plus-cli"
  },
  "dependencies": {
    "@casl/ability": "^3.1.2",
    "@casl/mongoose": "^2.3.1",
    "@feathers-plus/test-utils": "^0.3.6",
